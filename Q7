package task;

import java.util.LinkedList;
import java.util.List;

public class Task7 {
    // Main driver method
    public static void main(String[] args){
    	
        List<String> list = new LinkedList<String>();

        list.add("Rose");
        list.add("Jasmine");
        list.add("Sunflower");

        String[] arr = new String[list.size()];
        
        for (int i = 0; i < list.size(); i++)
            arr[i] = list.get(i);

        for (String x : arr)
            System.out.print(x + " ");
    }
}
